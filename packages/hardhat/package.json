{
  "name": "@collective-governance/hardhat",
  "description": "Fully Decentralized on-chain voting environment",
  "version": "0.0.1-rc.2",
  "main": "./index.js",
  "module": "./index.esm.js",
  "source": "./index.ts",
  "types": "./types/index.d.ts",
  "license": "MIT",
  "homepage": "https://github.com/KodexData/collective-governance#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KodexData/collective-governance.git"
  },
  "bugs": {
    "url": "https://github.com/KodexData/collective-governance/issues"
  },
  "contributors": [
    "pseudodeterminist <pseudodeterminist@kodexdata.systems> (https://kodexdata.systems)",
    "oldcryptogeek <oldcryptogeek@kodexdata.systems> (https://kodexdata.systems)",
    "wizardsorb <wizardsorb@kodexdata.systems> (https://kodexdata.systems)",
    "brittanneica <brittanneica@kodexdata.systems> (https://kodexdata.systems)"
  ],
  "keywords": [
    "ethers",
    "typescript",
    "openzeppelin",
    "kodexdata.systems",
    "collective-governance"
  ],
  "directories": {
    "test": "test",
    "contracts": "contracts",
    "helpers": "helpers",
    "scripts": "scripts",
    "tasks": "tasks"
  },
  "scripts": {
    "clean": "hardhat clean",
    "compile": "hardhat compile",
    "docgen": "hardhat docgen",
    "docs": "serve docs",
    "typings": "hardhat typechain",
    "test": "hardhat test --network hardhat",
    "deploy": "hardhat run scripts/deploy.ts",
    "testnet": "hardhat node",
    "cloc": "npx cloc --exclude-dir=$(tr '\n' ',' < .clocignore) . --exclude-ext=json,md",
    "pretty:sol": "prettier 'contracts/**/*.sol' --write",
    "pretty:types": "prettier 'typechain-types/**/*.ts' --write",
    "clean:all": "rimraf artifacts cache dist typechain-types types *.js *.d.ts",
    "rebuild": "npm run clean:all && npm run build",
    "clean:rollup": "rimraf types *.js *.esm.js",
    "build": "hardhat flat-build && rollup -c --bundleConfigAsCjs"
  },
  "peerDependencies": {
    "@ethersproject/abi": "*",
    "@ethersproject/bytes": "*",
    "@ethersproject/contracts": "*",
    "@ethersproject/keccak256": "*",
    "@ethersproject/providers": "*",
    "@ethersproject/strings": "*",
    "@kodex-data/flat-zeppelin": "*",
    "@kodex-data/prototypes": "*",
    "@kodex-data/testing": "*",
    "@kodex-data/utils": "*",
    "@types/node": "*",
    "mocha": "*"
  },
  "dependencies": {
    "ethers": "^5.7.2"
  },
  "devDependencies": {
    "@gnosis.pm/safe-contracts": "^1.3.0",
    "@kodex-data/flat-zeppelin": "^0.0.1-rc.4",
    "@kodex-data/prototypes": "^0.0.1-rc.7",
    "@kodex-data/testing": "^0.0.1-rc.6",
    "@kodex-data/utils": "^0.0.1-rc.5",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@openzeppelin/contracts": "^4.8.3",
    "@openzeppelin/hardhat-upgrades": "^1.23.1",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@typechain/ethers-v5": "^11.0.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/chai": "^4.3.5",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "chai": "^4.3.7",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.1",
    "hardhat": "^2.16.1",
    "hardhat-deploy": "^0.11.34",
    "hardhat-docgen": "^1.3.0",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-ignore-warnings": "^0.2.9",
    "json-colorizer": "^2.2.2",
    "mocha-logger": "^1.0.8",
    "prettier": "^3.0.0",
    "prettier-plugin-solidity": "^1.1.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "solidity-coverage": "^0.8.4",
    "ts-node": "^10.9.1",
